{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "global.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/context/socket.js", "src/app/ConferenceJoin/page.jsx", "src/app/ConferenceJoin/Conference.jsx", "src/app/ConferenceJoin/VideoRoom.jsx", "src/app/ConferenceJoin/VideoRoom.jsx", "src/app/ConferenceJoin/newPage.jsx", "src/app/api/agoraToken.js", "src/app/ConferenceJoin/[id]/VideoRoom.jsx", "src/app/conferenceTest/page.jsx", "src/app/api/socket/route.js"],
  "exclude": ["node_modules"]
}
